{
  "name": "app",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "esbuild": "^0.15.10",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "tailwindcss": "^3.1.8"
  },
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:js": "tsc && vite build",
    "lint": "eslint 'app/javascript/'",
    "format": "prettier --write app/javascript",
    "lint-fix": "eslint --cache --fix app/javascript/ && prettier --write app/javascript/",
    "codegen": "graphql-codegen --config codegen.yml",
    "preinstall": "typesync || :"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@types/eslint": "^8.4.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/tsc-watch": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "tsc-watch": "^5.0.3",
    "typescript": "*",
    "typesync": "^0.9.2",
    "vite": "^3.0.0",
    "vite-plugin-full-reload": "^1.0.4",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-ruby": "^3.1.0"
  }
}
